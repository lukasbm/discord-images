    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/node": "^18.7.8",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
